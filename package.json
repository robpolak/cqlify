{
  "name": "cqlify",
  "version": "0.0.16",
  "description": "Cassandra CQL ORM",
  "main": "app.js",
  "scripts": {
    "test": "node_modules/mocha/bin/mocha test --recursive --reporter spec",
    "unittest": "node_modules/mocha/bin/mocha test/unit --recursive --reporter spec",
    "cover_test": "node_modules/.bin/istanbul cover node_modules/mocha/bin/_mocha -- --reporter spec-xunit-file --recursive test",
    "cover_unittest": "node_modules/.bin/istanbul cover node_modules/mocha/bin/_mocha -- --reporter spec-xunit-file --recursive test/unit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/robpolak/cqlify.git"
  },
  "keywords": [
    "CQL",
    "ORM",
    "Cassandra",
    "C*"
  ],
  "author": "Robert Polak",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/robpolak/cqlify/issues"
  },
  "homepage": "https://github.com/robpolak/cqlify",
  "dependencies": {
    "cassandra-driver": "^2.0.1",
    "moment": "^2.10.2",
    "underscore": "^1.8.3",
    "validate.js": "^0.7.1"
  },
  "devDependencies": {
    "async": "^0.9.0",
    "chai": "^2.3.0",
    "mocha": "^2.2.4",
    "nodetime": "^0.8.15",
    "sinon": "^1.14.1",
    "istanbul": "^0.3.15",
    "spec-xunit-file": "0.0.1-3"
  }
}
